    <group name="SOLARPI">
        <group name="tools">
            <command name="" value="git status"/>
            <command name="" value="git add -A"/>
            <command name="" value="git pull"/>
            <command name="" value="git commit -m \"/>
            <command name="" value="rm -rf solarpi/"/>
            <command name="" value="rm -rf /tmp/solarpi_workspace/"/>
            <command name="" value="git clone https://github.com/wgabrys88/solarpi.git &#38;&#38; cd solarpi/"/>
            <command name="" value="./solarpi_deploy.sh"/>
            <command name="" value="crontab -l"/>
            <command name="" value="sudo vim \"/>
            <command name="" value="cat ~/.netrc"/>
            <command name="" value="cat /usr/local/etc/solarpi_config"/>
            <command name="" value="ls -la /tmp/solarpi_workspace/"/>
            <command name="" value="sudo systemctl stop solarpi-youtube.service"/>
            <command name="" value="sudo systemctl start solarpi-youtube.service"/>
            <command name="" value="systemctl status solarpi-youtube.service"/>
            <command name="" value="sudo systemctl stop solarpi-watchdog.timer"/>
            <command name="" value="sudo systemctl start solarpi-watchdog.timer"/>
            <command name="" value="systemctl status solarpi-watchdog.timer"/>
            <command name="" value="/usr/local/bin/file_execute.sh \"/>
            <command name="" value="/usr/local/bin/ftp_file_get.sh \"/>
            <command name="" value="/usr/local/bin/ftp_file_put.sh \"/>
            <command name="" value="/usr/local/bin/pwm_deinit.sh \"/>
            <command name="" value="/usr/local/bin/pwm_init.sh \"/>
            <command name="" value="/usr/local/bin/pwm_power_off.sh \"/>
            <command name="" value="/usr/local/bin/pwm_start.sh \"/>
            <command name="" value="/usr/local/bin/pwm_stop.sh \"/>
            <command name="" value="/usr/local/bin/relay_deinit.sh \"/>
            <command name="" value="/usr/local/bin/relay_init.sh \"/>
            <command name="" value="/usr/local/bin/relay_off.sh \"/>
            <command name="" value="/usr/local/bin/relay_on.sh \"/>
            <command name="" value="/usr/local/bin/system_deinit.sh \"/>
            <command name="" value="/usr/local/bin/system_pre_init.sh \"/>
            <command name="" value="/usr/local/bin/system_init.sh \"/>
            <command name="" value="/usr/local/bin/watchdog_job.sh"/>
            <command name="" value="/usr/local/etc/pwm_config.cfg \"/>
            <command name="" value="/usr/local/etc/solarpi_config.cfg \"/>
            <command name="" value="/usr/local/bin/hdmi_power_off.sh"/>
            <command name="" value="/usr/local/bin/hdmi_power_on.sh"/>
            <command name="" value="/usr/local/bin/usb_power_off.sh"/>
            <command name="" value="/usr/local/bin/usb_power_on.sh"/>
        </group>
        <group name="ftp">
            <command name="" value="ftp_file_put.sh \"/>
            <command name="" value="ftp_file_get.sh \"/>
            <command name="" value="file_execute.sh \"/>
        </group>
        <group name="relay20">
            <command name="" value="relay_init.sh 20"/>
            <command name="" value="relay_deinit.sh 20"/>
            <command name="" value="relay_on.sh 20"/>
            <command name="" value="relay_off.sh 20"/>
        </group>
        <group name="relay21">
            <command name="" value="relay_init.sh 21"/>
            <command name="" value="relay_deinit.sh 21"/>
            <command name="" value="relay_on.sh 21"/>
            <command name="" value="relay_off.sh 21"/>
        </group>
        <group name="pwm0">
            <command name="" value="pwm_init.sh 0"/>
            <command name="" value="pwm_deinit.sh 0"/>
            <command name="" value="pwm_start.sh 0 2000000"/>
            <command name="" value="pwm_start.sh 0 1000000"/>
            <command name="" value="pwm_stop.sh 0"/>
        </group>
        <group name="pwm1">
            <command name="" value="pwm_init.sh 1"/>
            <command name="" value="pwm_deinit.sh 1"/>
            <command name="" value="pwm_start.sh 1 2000000"/>
            <command name="" value="pwm_start.sh 1 1000000"/>
            <command name="" value="pwm_stop.sh 1"/>
        </group>
        <group name="system">
            <command name="" value="system_init.sh"/>
            <command name="" value="system_deinit.sh"/>
            <command name="" value="watchdog_enable.sh"/>
            <command name="" value="watchdog_disable.sh"/>
        </group>
    </group>